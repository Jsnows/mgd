    "iview": "^2.8.0",
    "marked": "^0.3.12",
    "vue": "^2.3.3",
    "vue-electron": "^1.0.6",
    "vue-router": "^2.5.3",
    "vue2-editor": "^2.3.11",
    "vuex": "^2.3.1",
    "winreg": "^1.2.4"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
