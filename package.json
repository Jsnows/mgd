{
  "name": "markdown",
  "productName": "markdown编辑器",
  "version": "1.0.0",
  "author": "崔琼雪 <cuiqiongxue@doumi.com>",
  "description": "An electron-vue project",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node complier/build.js",
    "build:mac": "cross-env BUILD_TARGET=darwin node complier/build.js",
    "build:linux": "cross-env BUILD_TARGET=linux node complier/build.js",
    "build:mas": "cross-env BUILD_TARGET=mas node complier/build.js",
    "build:win32": "cross-env BUILD_TARGET=win32 node complier/build.js",
    "build:clean": "cross-env BUILD_TARGET=clean node complier/build.js",
    "build:web": "cross-env BUILD_TARGET=web node complier/build.js",
    "dev": "node complier/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config complier/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config complier/webpack.renderer.config.js",
    "postinstall": ""
  },
  "dependencies": {
    "axios": "^0.16.2",
